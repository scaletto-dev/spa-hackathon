{
  "version": 2,
  "name": "spa-hackathon-frontend",
  "framework": null,
  "buildCommand": "npm run build -w @spa-hackathon/frontend && node deploy/vercel/scripts/build-output.js",
  "outputDirectory": ".vercel/output",
  "installCommand": "npm install --workspace=@spa-hackathon/frontend",
  "devCommand": "npm run dev -w @spa-hackathon/frontend",
  "regions": ["sin1"],
  "cleanUrls": true,
  "trailingSlash": false,
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/frontend",
  "env": {
    "VITE_API_URL": "YOUR_BACKEND_API_URL",
    "VITE_SUPABASE_URL": "YOUR_SUPABASE_URL",
    "VITE_SUPABASE_ANON_KEY": "YOUR_SUPABASE_ANON_KEY",
    "VITE_APP_ENV": "production",
    "VITE_APP_VERSION": "1.0.0"
  }
}
